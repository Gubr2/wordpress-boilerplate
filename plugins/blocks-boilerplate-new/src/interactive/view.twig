<div
	class="test"
	{{ fn('get_block_wrapper_attributes') }}
	data-wp-interactive="create-block"
  {{ fn('wp_interactivity_data_wp_context', arr) }}
	data-wp-watch="callbacks.logIsOpen"
>
	<button
		data-wp-on--click="actions.toggle"
		data-wp-bind--aria-expanded="context.isOpen"
		aria-controls="{{ fn('esc_attr', unique_id ) }}"
	>
		Toggle
	</button>

	<p
		id="{{ fn('esc_attr', unique_id ) }}"
		data-wp-bind--hidden="!context.isOpen"
	>
		Blocks Boilerplate New - hello from an interactive block!
	</p>
</div>