{
  "$schema": "https://schemas.wp.org/trunk/theme.json", 
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "spacing": {
			"units": ["rem"]
		},
    "layout": {
      "contentSize": "840px"
    },
    "color": {
      "defaultPalette": false,
      "background": false,
      "text": false,
      "link": false,
      "palette": [
        {
          "slug": "primary",
          "color": "#000",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#fff",
          "name": "Secondary"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
					"slug": "system-fonts",
					"name": "System fonts"
				}
			],
      "customFontSize": false,
      "fontWeight": false,
      "fontStyle": false,
      "fontSizes": []
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--system-fonts)"
    }
  }
}